{
  "editor.formatOnSave": true,
  "explorer.expandSingleFolderWorkspaces": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.glyphMargin": false,
  "eslint.validate": [],
  "eslint.format.enable": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "solidity.compileUsingRemoteVersion": "v0.8.23+commit.f704f362",
  "solidity.packageDefaultDependenciesContractsDirectory": "smart-contract/contracts",
  "solidity.packageDefaultDependenciesDirectory": "smart-contract/lib",
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "solidity.formatter": "forge",
  "solidity.defaultCompiler": "remote",
  "[typescriptreact]": {
    "editor.tabSize": 2
  },
  "editor.guides.indentation": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  }
}
